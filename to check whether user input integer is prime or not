#include<iostream.h>
#include<conio.h>

void main() {
	clrscr();

	int num;
	int i;

	cout<<"Enter any integer number"<<endl;
	cin>>num;

	for (i=2; i<=num-1 ; i++)

	{
		if (num%i==0)
			break;
	}

	{
		if (i==num)
			cout<<"Prime number"<<endl;

		else


			cout<<"NOT a Prime number"<<endl;
		getch();


	}

}
